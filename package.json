{
  "name": "news-assignment-api",
  "version": "1.0.0",
  "description": "api data for news assignment",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "start": "env-cmd -f .env tsc-watch --onSuccess \"node ./dist/server.js\"",
    "start:prod": "env-cmd -f .env.production tsc-watch --onSuccess \"node ./dist/server.js\"",
    "build": "node node_modules/typescript/bin/tsc",
    "build:prod": "env-cmd -f .env.production node node_modules/typescript/bin/tsc",
    "tsoa:gen": "tsoa routes -c ./tsoa.json && tsoa swagger -c ./tsoa.json"
  },
  "author": "Anupong Saelim",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dateformat": "^3.0.3",
    "env-cmd": "^10.0.1",
    "express": "^4.17.1",
    "fp-ts": "^2.0.5",
    "http": "0.0.0",
    "http-status-codes": "^1.4.0",
    "inversify": "^5.0.1",
    "jsonwebtoken": "8.5.1",
    "moment": "2.25.3",
    "mysql": "2.18.1",
    "swagger-ui-express": "^4.0.7",
    "tsoa": "^2.4.11",
    "typescript-string-operations": "^1.3.1",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^1.0.0",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.2",
    "@types/swagger-ui-express": "^3.0.1",
    "jest": "^24.8.0",
    "jest-junit": "^10.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tsc-watch": "^2.4.0",
    "typescript": "^3.5.3"
  }
}
